                            oPressed = true;
                        }
                        break;

                    case KeyEvent.KEY_RELEASED:
                        if (ke.getKeyCode() == KeyEvent.VK_O) {
                            oPressed = false;
                        }
                        break;
                    }
                    return false;
                }
            }
        });
    }
}
